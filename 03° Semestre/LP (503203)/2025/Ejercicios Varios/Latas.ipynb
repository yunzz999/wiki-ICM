   "source": [
    "def triangular(n):\n",
    "    return (n*(n+1))//2\n",
    "\n",
    "def main():\n",
    "    n=int(input())\n",
    "    j=0\n",
    "    t=1\n",
    "    while(t<n):\n",
    "        j+=1\n",
    "        t=triangular(j)\n",
    "\n",
    "    if(t==n):\n",
    "        print(\"se puede\")\n",
    "\n",
    "    else:\n",
    "        print(\"No apilables\",j-1,n-triangular(j-1))\n",
    "\n",
    "main()"
   ]
